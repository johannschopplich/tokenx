{
  "name": "tokenx",
  "type": "module",
  "version": "0.3.6",
  "packageManager": "pnpm@9.5.0",
  "description": "GPT token estimation and context size utilities without a full tokenizer",
  "author": "Johann Schopplich <hello@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/tokenx#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/tokenx.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/tokenx/issues"
  },
  "keywords": [
    "ai",
    "gpt",
    "token",
    "tiktoken"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "docs:generate": "tsx scripts/generateTable.ts",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:types": "tsc --noEmit",
    "release": "bumpp"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.3",
    "@types/node": "^20.14.10",
    "bumpp": "^9.4.1",
    "eslint": "^9.6.0",
    "gpt-tokenizer": "^2.1.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "unbuild": "^3.0.0-rc.6",
    "vitest": "^1.6.0"
  }
}
